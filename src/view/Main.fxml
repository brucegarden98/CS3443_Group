<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.MainController">
   <children>
      <GridPane prefHeight="800.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="115.0" minWidth="10.0" prefWidth="81.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="180.0" minWidth="10.0" prefWidth="135.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="162.0" minWidth="10.0" prefWidth="145.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="126.0" minWidth="10.0" prefWidth="122.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="edit_Main" mnemonicParsing="false" onAction="#handleEdit" text="Edit Profile" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="logout_Main" mnemonicParsing="false" onAction="#handleLogout" text="Logout" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <Button fx:id="food_Main" mnemonicParsing="false" onAction="#handleFood" text="Add Food" GridPane.columnIndex="3" GridPane.rowIndex="3" />
            <Text fx:id="welcome_Main" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome Back,XXXXX" />
            <Text fx:id="lastTime_Main" strokeType="OUTSIDE" strokeWidth="0.0" text="Last Login: 1/1/1111" GridPane.columnIndex="2" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Goal:" GridPane.rowIndex="1" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Current:" GridPane.rowIndex="2" />
            <Text fx:id="goalFeedback_Main" fill="#e10f0f" strokeType="OUTSIDE" strokeWidth="0.0" text="10000" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Text fx:id="currentFeedback_Main" fill="#0f13dd" strokeType="OUTSIDE" strokeWidth="0.0" text="200" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Text fx:id="advise_Main" strokeType="OUTSIDE" strokeWidth="0.0" text="Advise: XXXX" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Apple" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
